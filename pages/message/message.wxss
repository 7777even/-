.list {
  
width:90%;
margin: auto;
margin-top: 10rpx;
background-color: #fff;
box-shadow: 0 4rpx 24rpx 0 rgb(190, 190, 190);
padding:20rpx;

border-radius: 20rpx;
}
.inter{
  display: flex;
  justify-content: space-between;
  width:90%;
  margin: auto;
}
.list image{
  width:100rpx;
}
.text{
  font-size: 30rpx;
  text-align: center;

}
.liststyle{
  margin-bottom: 30rpx;
  height:30rpx
}
.face{
  width:90rpx;
  height:90rpx;
  border-radius: 50%;
  margin-right: 30rpx;
}
.user-list{
  display: flex;
  flex-direction: row;
  /* margin:30rpx 20rpx 0rpx 20rpx; */
margin-left: 20rpx;


  margin-bottom: 30rpx;
}
.nick-name{
margin-top: 22rpx;
font-size: 33rpx;
}
.role{
  margin-top: 25rpx;
  font-size: 28rpx;
  background-color:var(--themeColor);
  margin-left: 30rpx;
  border-radius: 10rpx;
  height:30rpx;
  line-height: 30rpx;
  padding: 8rpx;
  color: white;
 
}
.cancle{
width:120rpx;
height:40rpx;
line-height: 40rpx;
margin-top: 25rpx;

border-radius: 15rpx;
margin-left: auto;
margin-right:20rpx;
text-align: center;
}
.van-action-sheet__header{
  position: fixed;
  background-color: #fff;
  width:100%;
  border-radius: 25rpx;
}
.chatmess{
  width:98%;
  margin: auto;
  margin-top: 30rpx;
 
}
.divLine{
  background: #E0E3DA;
  width: 95%;
  height: 2rpx;
  margin:auto;
  margin-top: 10rpx;
 }
